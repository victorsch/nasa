<script>
  (function() {
    var buttons = document.querySelectorAll(".dashboard_details_indicator")
    // We could use the whole item as a button?
    // var buttons = document.querySelectorAll(".dashboard_menu li")
    var details = document.querySelectorAll(".dashboard_details")
    var linkIndicator = document.querySelector(".dashboard_link_indicator")
    var linkIndicatorMobile = document.querySelector(".dashboard_link_indicator_mobile")
  
    // Hide any details elements that might be open
    function hideAllDetails() {
      details.forEach(function(item) {
        item.classList.remove("dashboard_details__active")
      })
  
      // Show the link indicator
      if (linkIndicator)       linkIndicator.style.visibility       = "visible"
      if (linkIndicatorMobile) linkIndicatorMobile.style.visibility = "visible"
    }
  
    // If we have all of the elements we expect to have
    if (buttons.length > 0 && buttons.length === details.length) {
      console.dir(buttons)
      var index = 0
  
      // Make each of the buttons respond to a press by showing the related details element
      buttons.forEach(function(button) {
        var relatedDetails = details[index++]
        button.addEventListener("click", function(e) {
  
          hideAllDetails()
  
          // Hide the link indicator, since it’s super close to the details element
          if (linkIndicator)       linkIndicator.style.visibility       = "hidden"
          if (linkIndicatorMobile) linkIndicatorMobile.style.visibility = "hidden"
  
          // Show the details element
          relatedDetails.classList.add("dashboard_details__active")
  
          // Stop this event from propogating up to the link element,
          // since that will trigger the full-page overlay
          if (e) e.stopPropagation()
          if (e) e.preventDefault()
        })
      })
  
      // Turn each details element into a giant button that closes it
      details.forEach(function(next) {
        next.addEventListener("click", hideAllDetails)
      })
    }
  })()
</script>

