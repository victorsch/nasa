      var slidesShown = $(".explore_items").slick('slickGetOption', 'slidesToShow');
      var numberOfSlides = $(".explore_items").find('.slick-slide').length;
      if (slidesShown === numberOfSlides) {
        $(".explore_items").find('.slick-track').css('transform', 'translate3d(0px, 0px, 0px)');
