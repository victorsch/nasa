			<script language="javascript" type="text/javascript">
			function SubmitByEnter(e,txtQueryID,btnID) {
				if(e && (e.keyCode == 13)) {
					var txt = document.getElementById(txtQueryID);
					if(txt && (Trim(txt.value) != "")) {
						var btn = document.getElementById(btnID);
						if(btn != null) {
							var btnName = btn.name;
							if(btnName != "" ) {
								ShowImgLoading(true);
								__doPostBack(btnName,'');
							}
						}
					}
					else {
						alert("Please enter keyword(s) for search.");
						return false;
					}
				}
				return true;
			}
						function Trim(str) {
				var start;
				var end;
				str = str.toString();
				var len = str.length;
				for (start = 0; start < len; start ++)
				{
					ch = str.charAt(start);
					if (ch!=' ' && ch!='\t' && ch!='\n' && ch!='\r' && ch!='\f')
						break;
				}
				if (start == len)
		  			return "";
				for (end = len - 1; end > start; end --)
				{
					ch = str.charAt(end);
					if (ch!=' ' && ch!='\t' && ch!='\n' && ch!='\r' && ch!='\f')
						break;
				}
				end ++;
				return str.substring(start, end);
			}

			function ShowImgLoading(v) {
				var img = document.getElementById("imgLoading");
				if(img) {
					if(v) {
						img.style.display = "inline";
					}
					else
					{
						img.style.display = "none";
					}
				}
				return true;
			}

