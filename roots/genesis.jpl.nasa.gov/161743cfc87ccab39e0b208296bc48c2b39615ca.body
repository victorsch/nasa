<script>
    $.ajax({
        dataType: "json",
        url: "https://genesis.jpl.nasa.gov:8000/status",
        success: function(data, textStatus, jqXHR ) {
            let totals = $('#agape-totals-2814').get(0);
            let content = "Filter and download level 1-2 occultation data with our AGAPE tool. Serving {bytes_h} of data from {files} files spanning {occultations} occultations over {years} years.";
            for (const [key, val] of Object.entries(data['totals'])) {
                content = content.replace(
                    `{${key}}`,
                    Number.isInteger(val) ? val.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",") : val
                );
            }
            totals.innerHTML = content;
        }
    });
</script>

