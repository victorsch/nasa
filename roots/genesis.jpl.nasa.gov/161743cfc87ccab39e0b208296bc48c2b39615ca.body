<script src="https://cdnjs.cloudflare.com/ajax/libs/luxon/1.27.0/luxon.min.js" integrity="sha512-cQXElo4AdS11Wtr5GFBmliue0OBZIhJjcSOSmF3RASBNfJMrkhF/F5JknHUm9klwHLLVG5+oiV6INy/yjgYPoA==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>

<script>
    $.ajax({
        dataType: "json",
        url: "https://genesis.jpl.nasa.gov:8000/status",
        success: function(data, textStatus, jqXHR ) {
            let status = $('#agape-status-2822');
            if (data["last_data"] || data['last_sync']) {
                let syncTime = luxon.DateTime.fromISO(data["last_data"] ? data["last_data"] : data["last_sync"]);
                status.html(
                    `<span class="agape status time">${syncTime.toLocaleString(luxon.DateTime.DATETIME_FULL_WITH_SECONDS)}</span>`
                );
            }
            else {
                status.html(
                    `<span class="agape status time">No Update Data</span>`
                );
            }
            let tbl = '';
            tbl += '<table class="table table-sm table-hover table-striped agape status">';
            tbl += '<thead>';
            tbl += '<tr>';
            tbl += '<th scope="col">Platform</th>';
            tbl += '<th scope="col">New Occultations</th>';
            tbl += '<th scope="col">New Files</th>';
            tbl += '</tr>';
            tbl += '</thead>';
            tbl += '<tbody>';
            for (const [platform, counts] of Object.entries(data['platforms'])) {
                
                tbl += `<tr class="platform-txt-${platform}">`;
                tbl += `<th scope="row">${platform}</th>`;
                tbl += `<td>${counts.occultations}</td>`;
                tbl += `<td>${counts.files}</td>`;
                tbl += '</tr>';
            }
            tbl += '</tbody>';
            tbl += '</table>';
            status.append(tbl);
        }
    });
</script>



