<script>
	// Make sure the last filter in each group isn't clickable
	$(".recent_pages_gallery_form_filter_button:checked").each(function () {
		const group = $(this).attr("data-filter-group")
		if ($(`.recent_pages_gallery_form_filter_button:checked[data-filter-group=${group}]`).length <= $(this).attr("data-filter-group-min-count")) {
			$(this).prop("disabled", true);
			$(this).siblings("input").prop("disabled", true);
			$(this).siblings("label").css({"opacity": "0.65"});
			$(this).append(`<input class="visually-hidden" type=${this.type} name=${this.name} checked>`)
		}
	})

	// Add button functionality
	$(".recent_pages_gallery_form_filter_button").change(function () {
		const checked = !$(this).is(':checked');
		const group = $(this).attr("data-filter-group")
		if (checked) {
			// Make sure that this button is *actually* checked (since it just got un-checked by the click)
			$(this).prop("checked", true);

			// Uncheck any other filter buttons (in the same group) and disable their clear buttons
			$(`.recent_pages_gallery_form_filter_button:checked[data-filter-group=${group}]`).not(this).each(function () {
				$(this).prop("checked", false);
				$(this).siblings(".recent_pages_gallery_form_clear").prop("checked", false);
				$(this).siblings(".recent_pages_gallery_form_clear").prop("disabled", true);
			});
		} else {
			// Handle this button and its clear button
			$(this).prop("checked", true);
			$(this).siblings(`.recent_pages_gallery_form_clear`).each(function () {
				$(this).prop("checked", true);
				$(this).prop("disabled", false);
			});

			// Make sure any other disabled buttons (in the same group) are un-disabled
			$(`.recent_pages_gallery_form_filter_button:disabled[data-filter-group=${group}]`).each(function () {
				$(this).siblings("label").css({"opacity": "1"});
			});
		}
		$("#recent_pages_gallery_form").submit();
	});
</script>

