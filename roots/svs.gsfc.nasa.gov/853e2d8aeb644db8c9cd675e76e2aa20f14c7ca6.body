		<script>
			// Do the initial fetch
			$.ajax({
				type: "GET",
				url: "/api/recent/display?id=recent_pages_gallery",
				data: $("#recent_pages_gallery_form").serialize(),
				success: function (data) {
					const gallery_html = data['gallery_html'].trim()

					// Update the gallery list, fade out the loading overlay
					$("#recent_pages_gallery_wrapper_gallery").html(gallery_html);
					$("#recent_pages_gallery_wrapper_loading").fadeOut(500);
					$("#recent_pages_gallery").scroll(
						_.throttle(
							function () {
								handle_gallery(this);
							},
							100
						)
					);
					$("#recent_pages_gallery").scroll();
					handle_gallery_cards();
				},
				error: function () {
					$("#recent_pages_gallery_wrapper_loading").html("There was an error fetching this. Please refresh the page and try again.");
				},
			})

			// Binds filter functionality
			$("#recent_pages_gallery_form").submit(function (e) {
				e.preventDefault();

				// Prevents the user from deselecting filters while the gallery is still responding to their last change
				$("#recent_pages_gallery_form").find("input[id$='_clear']").prop("disabled", true);

				$("#recent_pages_gallery_wrapper_loading").fadeIn(500, function () {
					$.ajax({
						type: "GET",
						url: "/api/recent/display?id=recent_pages_gallery",
						data: $("#recent_pages_gallery_form").serialize(),
						success: function (data) {
							const filter_html = data['filter_html'].trim()
							const gallery_html = data['gallery_html'].trim()
							// Update the filter list
							$("#recent_pages_gallery_form").html(filter_html);

							// Update the gallery list, fade out the loading overlay
							$("#recent_pages_gallery_wrapper_gallery").html(gallery_html);
							$("#recent_pages_gallery_wrapper_loading").fadeOut(500);
							$("#recent_pages_gallery").scroll(
								_.throttle(
									function () {
										handle_gallery(this);
									},
									100
								)
							);
							$("#recent_pages_gallery").scroll();
							handle_gallery_cards();
						},
						error: function () {
							$("#recent_pages_gallery_wrapper_loading").html("There was an error fetching this. Please refresh the page and try again.");
						},
					})
				});
			});
		</script>
