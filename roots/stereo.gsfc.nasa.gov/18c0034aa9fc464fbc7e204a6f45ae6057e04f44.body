<script type="text/javascript">
$(function() {    
    makeRequest("/news/news.xml",newsParser);
    makeRequest("/gallery/xml/selects.xml",selectsParser);
    function makeRequest(uri,func) {
        $.ajax({
            type: "GET",
            url: uri,
            dataType: "xml",
            success: func
        });
    }
    
    function newsParser(xml) {
        $(xml).find("item").each(function(i) {
            if(i<2) {
                var title = $(this).find("title").text();
                var link = $(this).find("link").text();            
                var description = $(this).find("description").text();
                var pubDate = $(this).find("pubDate").text();
                
                var html = '<li><span><strong><a href="'+link+'">'+title+'</a></strong></span><br />';
                html += '<span style="font-weight:bold">'+pubDate+'</span>';
                html += '<p style="margin:5px 0 0 0">'+description+'</p></li>';                
                $('<ul />').attr('class','list').appendTo('#newswrapper').append(html);
            }
        });
    }
    
    function selectsParser(xml) {
        $(xml).find("item").each(function(i) {
            if(i<1) {
                var id = $(this).attr('id');
                var title = $(this).find("title").text();
                var link = $(this).find("link").text();            
                var description = $(this).find("description").text();
                var thumbnail = $(this).find("thumbnail").text();
                
                var html = '<a href="'+link+'?id=selects&iid='+id+'"><img src="'+thumbnail+'" width="150" height="125" class="thumbnail" alt="image" /></a><br />';
                html += '<small>'+title+'</small>';
                $('#selectswrapper').html(html);
            }
        });
    }
});
</script>

