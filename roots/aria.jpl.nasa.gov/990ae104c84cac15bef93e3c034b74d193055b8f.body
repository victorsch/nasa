                <script>
                $(document).ready(function(){
                  $(".explore_items").utilCarousel({
                    responsiveMode : 'itemWidthRange',
                    itemWidthRange : eval([230, 380]),    //will keep item's width between x and y px and will calc how many items to show
                    pagination: true,   //shows navigation dots when pages present but seems necessary for the next/prev page
                    scrollPerPage: true,
                    rewind: false,
                    indexChanged : function(index) {
                      console.log(index.toString());
                      if (index == 0){
                        $("#explore_prev").addClass("disabled")
                      } else {
                        $("#explore_prev").removeClass("disabled")
                      }

                      if (index >= $(".explore_items").data('utilCarousel').pageIndexs[$(".explore_items").data('utilCarousel').pageIndexs.length - 1]){
                        $("#explore_next").addClass("disabled")
                      } else {
                        $("#explore_next").removeClass("disabled")
                      }
                    },
                    afterInit: function(){
                      util_carousel.setScrollForMobile($(".explore_items").data('utilCarousel'));
                      $(window).on('resize', function(){
                        util_carousel.setScrollForMobile($(".explore_items").data('utilCarousel'));
                      });
                    }
                  });


                  $("#explore_prev").click(function(){
                    $(".explore_items").data('utilCarousel').prev();
                  });

                  $("#explore_next").click(function(){
                    $(".explore_items").data('utilCarousel').next();
                  });

                });
                </script>
