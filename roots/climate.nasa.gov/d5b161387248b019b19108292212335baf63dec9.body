<script>
  var gmu = {
  
    addZero: function(i) {
      if (i < 10) {
        i = "0" + i;
      }
      return i;
    },
  
    checkGMU: function(){
      var param = mb_utils.getParam('pid')
      var cookie_exists = mb_utils.readCookie('GMU_Visitor')
      var now = new Date();
      var date_string = (now.getMonth()+1) + "/"
                + now.getDate() + "/" 
                + now.getFullYear() + " @ "  
                + now.getHours() + ":"  
                + gmu.addZero(now.getMinutes()) + ":" 
                + gmu.addZero(now.getSeconds()) + " ." 
                + now.getMilliseconds();
      var event_label = window.location.href + " (" + date_string + ")"
      //if(!cookie_exists && param && param.length > 0){
      console.log('param: ' + param);
      console.log('cookie_exists: ' + cookie_exists);
      if(param && param.length > 0 && cookie_exists != param){
        console.log('create GMU cookie')
        var date = new Date();
        date.setTime(date.getTime()+(10*60*1000));  //10 minutes
        var expires = "; expires="+date.toGMTString();
        document.cookie = "GMU_Visitor="+param+expires+'; path=/';
        _sendEvent('GMU', param, event_label, now.getTime());
      } else if (cookie_exists){
        console.log('use existing GMU cookie')
        _sendEvent('GMU', cookie_exists, event_label, now.getTime());
      } else {
        //do nothing
      }
    },
  
    analyticsReady: function(){
      if(typeof("_sendEvent") == 'undefined'){
        console.log("analytics not loaded. wait 300ms.")
        window.setTimeout(gmu.analyticsReady, 300);
      } else {
        console.log("analytics ok.")
        gmu.checkGMU()
      }
    }
  }
  
  $(document).ready(function(){
    gmu.analyticsReady();
  })
</script>

