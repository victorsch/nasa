function FWImage_Indigo(chain,msg)
{
	if (chain==1)
	{
		FWShowHideLayerFade("DSC0030crop",msg,msg,0.5);
		FWShowHideLayerFade("DSC0068crop",msg,msg,0.5);
		FWShowHideLayerFade("CNOFSSatellite",msg,msg,0.5);
	}
	else 
	{
		FWShowHideLayerFade("DSC0030crop",!msg,msg,0.5);
		FWShowHideLayerFade("DSC0068crop",!msg,msg,0.5);
		FWShowHideLayerFade("CNOFSSatellite",!msg,msg,0.5);
	}
	if (chain==2)
		FWShowHideLayerFade("VEFIorbit2554panelmay32008",msg,msg,0.5);
	else 
		FWShowHideLayerFade("VEFIorbit2554panelmay32008",!msg,msg,0.5);
	if (chain==3)
	{
		FWShowHideLayerFade("cnofsglobeimage",msg,msg,0.5);
		FWShowHideLayerFade("DSC0034crop",msg,msg,0.5);
	}
	else 
	{
		FWShowHideLayerFade("cnofsglobeimage",!msg,msg,0.5);
		FWShowHideLayerFade("DSC0034crop",!msg,msg,0.5);
	}
}
window.FT_Indigo=new Object;
window.FT_Indigo['0']=FWImage_Indigo;
