    function basicTitles()
    {
        //set all tooltips for urls to CEOS
        $("a").each(function(o)
        {
            var h = $(this).attr("href");
            var t = $(this).attr("title");
            
            if (h!=null && -1<h.indexOf("ceos.org") && (t==null || t.length1) )
            {
                $(this).attr("title", "This link will transfer you to the CEOS web site");
                _run_title_once = true;
            }
        });
        if (!_run_title_once){window.setTimeout("basicTitles()", 2000);}
    }

    $(document).ready(basicTitles);
}catch (e)
{
