        <script type="text/javascript">
        $=jQuery;
        
        var articles = $(".content");
        for (var i=0; i<articles.length; i++) {
          var e = articles[i];
          $("#article-container")[0].appendChild(e); 
        }

        var $outer_container = $("#outer_container");
        if (articles.length > 0) { $outer_container.css("display", "block"); }
        var $thumbScroller = $("#thumbScroller");
        var $thumbScroller_container = $("#thumbScroller .container");
        var $thumbScroller_content = $("#thumbScroller .content");
        var $thumbScroller_thumb = $("#thumbScroller .thumb");

        var padding = $outer_container.css("paddingRight").replace("px", "");
        var sliderWidth = $thumbScroller.width() - padding;
        var itemWidth = $thumbScroller_content.first().innerWidth();
        var totalContent = $thumbScroller_content.length * itemWidth;

        $thumbScroller_container.css("marginLeft", ($thumbScroller_thumb.first().innerWidth()/2) + "px"); 
        $thumbScroller_container.css("width", totalContent);
        $thumbScroller.css("width", sliderWidth);

        $thumbScroller.mousemove(function(e) {
          if($thumbScroller_container.width() > sliderWidth) {
            var mouseCoords  =  (e.pageX - this.offsetLeft);
            var mousePercentX = mouseCoords/sliderWidth;
            var destX = -(totalContent + itemWidth - sliderWidth - sliderWidth) * mousePercentX;
            var thePosA = mouseCoords - destX;
            var thePosB = destX - mouseCoords;
            var animSpeed = 600; //ease amount
            var easeType = "easeOutCirc";
            if(mouseCoords > destX) {
              $thumbScroller_container.stop().animate({left: -thePosA}, animSpeed, easeType);
            } else if(mouseCoords < destX) {
              $thumbScroller_container.stop().animate({left: thePosB}, animSpeed, easeType);
            } else {
              $thumbScroller_container.stop();
            }
          }});

        var fadeSpeed = 300;

        $thumbScroller_thumb.each(function () {
          var $this = $(this);
          $this.fadeTo(fadeSpeed, 0.8);
        });

        $thumbScroller_thumb.hover(
          function() {
            var $this = $(this);
            $this.stop().fadeTo(fadeSpeed, 1);
          },
          function() {
            var $this = $(this);
            $this.stop().fadeTo(fadeSpeed, 0.8);
          }
        );

        </script>

