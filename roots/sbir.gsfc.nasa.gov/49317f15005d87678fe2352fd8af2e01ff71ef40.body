  <div class="skin-default">
    
    <div id="views_slideshow_cycle_main_ioc_speakers_front_page_carousel-block_1_1" class="views_slideshow_cycle_main views_slideshow_main"><div id="views_slideshow_cycle_teaser_section_ioc_speakers_front_page_carousel-block_1_1" class="views-slideshow-cycle-main-frame views_slideshow_cycle_teaser_section">
