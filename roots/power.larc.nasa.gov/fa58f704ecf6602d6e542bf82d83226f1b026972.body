  <script type="text/javascript">
    function chbg(name, color) {
        document.getElementById(name).style.backgroundColor = color;
    }
    
    function expandBlue(el) {
      if (el.classList.contains('unexpanded-blue')) {
        el.classList.remove('unexpanded-blue');
        el.classList.add('expanded-blue');
      } else {
        el.classList.add('unexpanded-blue');
        el.classList.remove('expanded-blue');
      }
    }

    let isMobileScreen = false;
        // changes navbar layout depending if mobile device is detected
    if( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
      isMobileScreen = true;
    }
    window.onscroll = function() { scrollChangeNavbar() }
      function scrollChangeNavbar() {
        let marginType = (isMobileScreen) ? "40px" : "30px"; 
        // if page isn't long enough, leave
        if (document.documentElement.scrollHeight < 1100) return;
        // Modifies styles after scroll
        if (document.body.scrollTop > 25 || document.documentElement.scrollTop > 25){
          document.getElementById("mainNav").style.margin = "0px";
          //Goes back to initial styles when goes back to top of page
        } else {
          document.getElementById("mainNav").style.margin = marginType;
        }
      }
  </script>

