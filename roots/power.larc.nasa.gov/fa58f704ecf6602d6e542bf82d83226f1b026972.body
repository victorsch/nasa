  <script>
    $(function () {

      $("document").ready(function () {

        $('#register').click(function () {
          ga('send', 'event', 'Website', 'Button Click', 'Register');
        })
        $('#rel').click(function () {
          ga('send', 'event', 'Website', 'Button Click', 'Release Notes');
        })
        $('#pub').click(function () {
          ga('send', 'event', 'Website', 'Button Click', 'Publications');
        })
        $('#faq').click(function () {
          ga('send', 'event', 'Website', 'Button Click', 'FAQ');
        })
        $('#mth').click(function () {
          ga('send', 'event', 'Website', 'Button Click', 'Documentation');
        })

        var getAbsoluteUrl = (function () {
          var a;
          return function (url) {
            if (!a) a = document.createElement('a');
            a.href = url;
            return a.href;
          };
        })();

        parameters_json = getAbsoluteUrl('/api/system/manager/system/parameters')

        $.getJSON(parameters_json, function (data) {

        })
          .done(function (data) {
            
            parameters_json_data = data;

            for (parameter_abbreviation in parameters_json_data) {
              parameter = data[parameter_abbreviation]
              $('#parameterDictSelect').append('<option value="' + parameter_abbreviation + '">' + parameter["longname"] + '</option>');
            }
          })
          .fail(function (data) {
            $('#mount2').css('display', 'block')
          })
          .always(function (data) {
          });
      })

      $("#parameterDictSelect").change(function () {

        var userSelection = $("#parameterDictSelect option:selected").val();

        for (parameter_abbreviation in parameters_json_data) {
          parameter = parameters_json_data[parameter_abbreviation]
          $('#alias').css('display', 'block')
          $('#stat').css('display', 'block')
          $('#unit').css('display', 'block')
          if (parameter_abbreviation == userSelection) {
            $("#paramInfo").empty()
            $("#aliases").empty()
            $("#stats").empty()
            $("#units").empty()
            
            var paramInfo = "<h4><b>Name:</b> " + parameter["longname"] + "</h4>" + "<h4><b>Abbreviation:</b> " + userSelection + "</h4>" + "<h4><b>Definitions: </b></h4>" + "<p style=\"margin-left:5%;\"><b>Climatology:</b> " + parameter["definition"]['CLIMATOLOGY'] + "</p>" + "<p style=\"margin-left:5%;\"><b>Monthly & Annual:</b> " + parameter["definition"]['MONTHLY'] + "</p>" + "<p style=\"margin-left:5%;\"><b>Daily:</b> " + parameter["definition"]['DAILY'] + "</p>" + "<p style=\"margin-left:5%;\"><b>Hourly:</b> " + parameter["definition"]['HOURLY'] + "</p>"

            var aliasesInfo = "<h4>Aliases</h4>" + "<p>" + parameter["longname"] + "</p>"

            var statsInfo = "<h4>Statistics Available</h4>" + "<p>" + parameter["temporal"].join(", ") + "</p>"

            var unitsInfo = "<h4>Units Available</h4>" + "<p><b>Agroclimatology:</b> " + parameter["units"]["AG"] + "<br><b>Sustainable Buildings:</b> " + parameter["units"]["SB"] + "<br><b>Renewable Energy:</b> " + parameter["units"]["RE"] + "</p>"

            $(paramInfo).appendTo("#paramInfo");
            $(aliasesInfo).appendTo("#aliases");
            $(statsInfo).appendTo("#stats");
            $(unitsInfo).appendTo("#units");
          }
        }
      });
    });
  </script>

